// Noto Sans for San Serif Alphabet
@font-face
  font-family: 'Noto Sans'
  font-weight: 100
  font-weight: 200
  font-weight: 300
  font-weight: 400
  font-weight: 500
  src: local('NotoSans'),
       local('Noto Sans'),
       url(//fonts.gstatic.com/s/notosans/v6/LeFlHvsZjXu2c3ZRgBq9nJBw1xU1rKptJj_0jans920.woff2) format('woff2')
  font-display: swap
@font-face
  font-family: 'Noto Sans'
  font-weight: 600
  font-weight: 700
  font-weight: 800
  font-weight: 900
  src: local('NotoSans-Bold'),
       local('Noto Sans Bold'),
       url(//fonts.gstatic.com/s/notosans/v6/PIbvSEyHEdL91QLOQRnZ1xampu5_7CjHW5spxoeN3Vs.woff2) format('woff2')
  font-display: swap
